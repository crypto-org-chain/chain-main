        "@0x/protocol-utils": "^1.9.4",
        "@0x/subproviders": "^6.6.0",
        "@0x/utils": "^6.4.4",
        "@0x/web3-wrapper": "^7.6.1",
        "axios": "^0.25.0",
        "axios-mock-adapter": "^1.20.0",
