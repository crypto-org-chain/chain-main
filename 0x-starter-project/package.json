        "@types/lodash": "^4.14.137",
        "@types/websocket": "^0.0.40",
        "cross-env": "^5.1.4",
        "download-cli": "^1.0.5",
        "ethereum-types": "^3.0.0",
        "ganache-cli": "6.6.0",
        "make-promises-safe": "^5.0.0",
        "npm-run-all": "^4.1.2",
